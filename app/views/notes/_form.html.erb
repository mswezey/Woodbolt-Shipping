<% form_for @note do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :shipment_id %><br />
    <%= f.text_field :shipment_id %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :content %><br />
    <%= f.text_area :content %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
